    bytes32 constant EXPRESSION_DEPLOYER_DEPLOYED_CODEHASH =
        bytes32(0xc1407cd6436d9fac5721c1ac169e2c12ae908b99e9ac82b4147641d2f7c3df75);

    /// The address of the `RainterpreterDISPaiRegistry` contract when deployed
